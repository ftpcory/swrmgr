{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3Access",
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": [
                "arn:aws:s3:::REPLACE_BUCKET/REPLACE_NAME",
                "arn:aws:s3:::REPLACE_BUCKET/REPLACE_NAME/*"
            ]
        }
    ]
}
